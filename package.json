    "@grafana/eslint-config": "^7.0.0",
    "@grafana/tsconfig": "^1.3.0-rc1",
    "@swc/core": "^1.3.107",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.34",
    "@testing-library/jest-dom": "6.4.1",
    "@testing-library/react": "14.2.1",
    "@types/geojson": "^7946.0.14",
    "@types/glob": "^8.1.0",
